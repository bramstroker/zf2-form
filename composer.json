{
    "name": "stroker/form",
    "description": "ZF2 module for extending forms with live clientside validation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "jquery validate",
        "form",
        "validation",
        "zend framework 2",
        "module"
    ],
    "authors": [
        {
            "name": "Bram Gerritsen",
            "email": "bgerritsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6 || >= 7.0",
        "zendframework/zend-eventmanager": "~2",
        "zendframework/zend-form": "~2",
        "zendframework/zend-http": "~2",
        "zendframework/zend-json": "~2",
        "zendframework/zend-mvc": "~2",
        "zendframework/zend-cache": "~2",
        "zendframework/zend-config": "~2",
        "zendframework/zend-i18n": "~2",
        "zendframework/zend-i18n-resources": "~2",
        "zendframework/zend-inputfilter": "~2",
        "zendframework/zend-servicemanager": "~2",
        "zendframework/zend-validator": "~2",
        "zendframework/zend-view": "~2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/phpunit": "~5.5",
        "friendsofphp/php-cs-fixer": "~1.9"
    },
    "autoload": {
        "psr-0": {
            "StrokerForm": "src/",
            "StrokerFormTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
